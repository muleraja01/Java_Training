package basicSyntax;

public class sampleOperators {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a, b, c;
		a=10; b=20; c=30;
		System.out.println(a+b);
		System.out.println(a-b);
		System.out.println(a*b);
		System.out.println(a/b);
		System.out.println(a%b);
		
		System.out.println(a>b);
		System.out.println(a!=b);
		System.out.println(a==b);
		System.out.println();
	}

}
